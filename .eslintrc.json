{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "extends": ["eslint:recommended", "prettier", "plugin:sonar/recommended"],
  "env": {
    "es2021": true,
    "node": true
  },
  "plugins": ["sonar"],
  "rules": {
    "no-console": "off",
    "sonar/deprecation": 1
  }
}
